pipeline {
 agent any
      tools{
         maven 'Maven 3.8.6'
           jdk 'JDK 11'
}
stages {
   stage('Checkout'){
       steps {
          git 'https://github.com/ganesh-1234567/jenkins-maven.git'
         }
    }
    stage ('Build') {
          steps {
            sh 'mvn clean install'
             }
       }
    }
}
